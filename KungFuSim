_G.segg = "Egg1"
_G.egg = true
function egg()
     while _G.egg == true do
        game:GetService("ReplicatedStorage"):WaitForChild("PetSystem"):WaitForChild("Remotes"):WaitForChild("HatchPet"):FireServer(_G.segg, Egg1)
        wait(0)
    end
end

