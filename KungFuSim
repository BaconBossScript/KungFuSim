_G.click = true
_G.boss = true
_G.sboss = "1"
function boss() 
     while _G.boss == true do
        game:GetService("ReplicatedStorage"):WaitForChild("WallSystem"):WaitForChild("Remotes"):WaitForChild("StartAutoFight"):FireServer(_G.sboss, false)
     wait(0)
    end
end 

function click()
     while _G.click == true do
        game:GetService("ReplicatedStorage"):WaitForChild("WallSystem"):WaitForChild("Remotes"):WaitForChild("Attack"):FireServer()
     wait(0)
    end
end

Tp2Tab:AddToggle({
	Name = "Auto Click Fight",
	Default = false,
	Callback = function(Value)
        _G.click = Value
        click()
	end    
})

Tp2Tab:AddToggle({
	Name = "Auto Boss Fight",
	Default = false,
	Callback = function(Value)
        _G.boss = Value
        boss()
	end    
})

