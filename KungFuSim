_G.train = true
function train() 
     while _G.train == true do
        local args = {
            [1] = workspace:WaitForChild("Targets"):WaitForChild("TargetsZone4"):WaitForChild("skill"):WaitForChild("1"):WaitForChild("Cylinder")
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("WeaponSystem"):WaitForChild("Remotes"):WaitForChild("DummyHit"):FireServer(unpack(args))
     wait(0)
    end
end        

_G.train2 = true
function train2() 
     while _G.train2 == true do
        local args = {
            [1] = workspace:WaitForChild("Targets"):WaitForChild("TargetsZone4"):WaitForChild("skill"):WaitForChild("2"):WaitForChild("Cylinder.004")
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("WeaponSystem"):WaitForChild("Remotes"):WaitForChild("DummyHit"):FireServer(unpack(args))        
     wait(0)
    end
end      

